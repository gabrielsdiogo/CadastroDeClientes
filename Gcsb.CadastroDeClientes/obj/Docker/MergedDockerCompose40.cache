networks:
  gcsb-CadastroDeClientes-network: {}
services:
  gcsb-CadastroDeClientes-database:
    environment:
      POSTGRES_DB: postgres
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgres
    image: postgres:9.5
    networks:
      gcsb-CadastroDeClientes-network: {}
    ports:
    - published: 5432
      target: 5432
    restart: always
    volumes:
    - pgdata:/var/lib/postgresql/data:rw
  gcsb.cadastrodeclientes.webapi:
    build:
      args:
        FEED_URL: https://pkgs.dev.azure.com/globalwebcsb/_packaging/gcsbvivofeed/nuget/v3/index.json
      context: C:\Users\Gabri\GitHubPESSOAL\CadastroDeClientes\Gcsb.CadastroDeClientes
      dockerfile: Gcsb.CadastroDeClientes.WebApi/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Gcsb.CadastroDeClientes.WebApi
      target: base
    container_name: Gcsb.CadastroDeClientes.WebApi
    depends_on:
      gcsb-CadastroDeClientes-database:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ALLOWED_HOSTS: localhost
      API_DESCRIPTION: '"Cadastro De Clientes endpoints"'
      API_TITLE: '"Gcsb.CadastroDeClientes.Api"'
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_Kestrel__Certificates__Default__Password: docker1234
      ASPNETCORE_Kestrel__Certificates__Default__Path: /https/aspnetapp.pfx
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      BASE_URI_AUTH: '"https://vivo-int-dev.vivoplataformadigital.com.br"'
      DBCONN: '"Host=localhost;Port=5432;Database=postgres;User Id=postgres;Password=postgres;"'
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      JWT_KEY: '"b508f4f3-1f2b-4261-a60a-121b696be82a"'
      NUGET_FALLBACK_PACKAGES: ''
      TRAEFIK_PATH: '""'
    image: gcsbcadastrodeclienteswebapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/Gcsb.CadastroDeClientes.WebApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      gcsb-CadastroDeClientes-network: {}
    ports:
    - published: 8080
      target: 80
    - published: 443
      target: 443
    tty: true
    volumes:
    - C:\Users\Gabri\GitHubPESSOAL\CadastroDeClientes\Gcsb.CadastroDeClientes\Gcsb.CadastroDeClientes.WebApi:/app:rw
    - C:\Users\Gabri\.aspnet\https:/https:ro
    - C:\Users\Gabri\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Gabri\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Gabri\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Gabri\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Gabri\GitHubPESSOAL\CadastroDeClientes\Gcsb.CadastroDeClientes:/src:rw
version: '3.7'
volumes:
  pgdata: {}