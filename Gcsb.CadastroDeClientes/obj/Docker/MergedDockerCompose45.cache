networks:
  gcsb-CadastroDeClientes-network: {}
services:
  gcsb-CadastroDeClientes-database:
    environment:
      POSTGRES_DB: postgres
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgres
    image: postgres:9.5
    networks:
      gcsb-CadastroDeClientes-network: {}
    ports:
    - published: 5432
      target: 5432
    restart: always
    volumes:
    - pgdata:/var/lib/postgresql/data:rw
  gcsb.cadastrodeclientes.webapi:
    build:
      args:
        FEED_URL: https://pkgs.dev.azure.com/globalwebcsb/_packaging/gcsbvivofeed/nuget/v3/index.json
      context: C:\Users\Gabri\GitHubPESSOAL\CadastroDeClientes\Gcsb.CadastroDeClientes
      dockerfile: Gcsb.CadastroDeClientes.WebApi/Dockerfile
    depends_on:
      gcsb-CadastroDeClientes-database:
        condition: service_started
    environment:
      ALLOWED_HOSTS: localhost
      API_DESCRIPTION: '"Cadastro De Clientes endpoints"'
      API_TITLE: '"Gcsb.CadastroDeClientes.Api"'
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_Kestrel__Certificates__Default__Password: docker1234
      ASPNETCORE_Kestrel__Certificates__Default__Path: /https/aspnetapp.pfx
      ASPNETCORE_URLS: https://+:443;http://+:80
      BASE_URI_AUTH: '"https://vivo-int-dev.vivoplataformadigital.com.br"'
      DBCONN: Host=gcsb-CadastroDeClientes-database;Port=5432;Database=postgres;User
        Id=postgres;Password=postgres;
      JWT_KEY: '"b508f4f3-1f2b-4261-a60a-121b696be82a"'
      TRAEFIK_PATH: '""'
    image: gcsbcadastrodeclienteswebapi
    networks:
      gcsb-CadastroDeClientes-network: {}
    ports:
    - published: 8080
      target: 80
    - published: 443
      target: 443
    volumes:
    - C:\Users\Gabri\.aspnet\https:/https:ro
version: '3.7'
volumes:
  pgdata: {}